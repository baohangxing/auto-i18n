{
  "name": "@h1mple/auto-i18n",
  "displayName": "auto-i18n",
  "version": "0.2.7",
  "private": true,
  "description": "A vscode extension helps you transform your project to an i18n-project automatically",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "auto-i18n.transformCompare",
        "title": "Compare Text with content i18n transformed",
        "category": "auto-i18n"
      },
      {
        "command": "auto-i18n.transform",
        "title": "Transform content i18n",
        "category": "auto-i18n"
      },
      {
        "command": "auto-i18n.revertCompare",
        "title": "Compare Text with content i18n reverted",
        "category": "auto-i18n"
      },
      {
        "command": "auto-i18n.revert",
        "title": "Revert i18n content transformed",
        "category": "auto-i18n"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "build": "pnpm run package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@h1mple/auto-i18n-core": "workspace:^",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0",
    "vscode-extension-telemetry": "^0.1.7"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.2",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
