{
  "name": "@h1mple/auto-i18n-cli",
  "version": "0.2.8",
  "description": "A CLI tool helps you transform your project to an i18n-project automatically.",
  "author": "H1mple<https://github.com/baohangxing>",
  "license": "MIT",
  "homepage": "https://github.com/baohangxing/auto-i18n/packages/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baohangxing/auto-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/baohangxing/auto-i18n/issues"
  },
  "keywords": [
    "vue",
    "i18n",
    "ts",
    "auto",
    "cli",
    "h1mple"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "auto-i18n": "bin/index.mjs"
  },
  "files": [
    "dist",
    "bin",
    "global.d.ts",
    "README.md",
    "demo"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "unbuild",
    "test": "vitest  --no-threads",
    "coverage": "vitest run --coverage  --no-threads"
  },
  "dependencies": {
    "@h1mple/auto-i18n-core": "workspace:^",
    "ansi-colors": "^4.1.3",
    "chalk": "^5.2.0",
    "cli-progress": "^3.12.0",
    "commander": "^9.4.1",
    "cosmiconfig": "^8.0.0",
    "fs-extra": "^11.1.0",
    "node-xlsx": "^0.21.0",
    "serialize-javascript": "^6.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/serialize-javascript": "^5.0.2",
    "@vitest/coverage-c8": "^0.28.4",
    "rimraf": "^3.0.2",
    "vitest": "^0.28.4"
  }
}
